//do while loops
#include<stdio.h>
int main(){
    int i=1;
    do{
        printf("%d\n",i);
        i++;
    } while(i<=5);
    return 0;
}
//do while loops( reverse order)
#include<stdio.h>
int main(){
    int i=5;
    do{
        printf("%d\n",i);
        i--;
    } while(i>=1);
    return 0;
}
//sum of first n natural no
#include<stdio.h>
int main(){
    int no;
    printf("enter the no:");
    scanf("%d",&no);
    int sum = 0;
    for(int i=1;i<=no;i++){
        sum=sum+i;
    }
    printf("sum is %d",sum);
return 0;
    
}
